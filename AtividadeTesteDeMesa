Exercício 10:

$cadeia = readline("Diga uma frase: ");
print "\n";
$caracter = readline("Diga o caracter que deseja subtrair ");
print "\n";

tira_carater ($cadeia, $caracter);

function tira_carater (&$cadeia, &$caracter){
   $tamanho = mb_strlen($cadeia);

   $cadeia = str_ireplace($caracter, "", $cadeia);

   $tamanho_agora = mb_strlen($cadeia);
   $numero_caracter = $tamanho - $tamanho_agora;

   print "A frase agora é: $frase \n";
   print "Foram retirados $numero_caracter caracteres \"$caracter\" da frase.";
}
